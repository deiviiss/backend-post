{
  "name": "mern-context-server",
  "version": "1.0.0",
  "description": "Servidor de express para aplicaci√≥n del curso Context",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon server/src/index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "migrations:debug": "sequelize-cli db:migrate --debug"
  },
  "keywords": [
    "NodeJs",
    "Express"
  ],
  "author": "David Hilera <david.hilera@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.30.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "fs-extra": "^10.1.0",
    "http-errors": "^2.0.0",
    "mongoose": "^6.3.6",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.16.1",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2"
  }
}
